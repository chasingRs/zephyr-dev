config BOARD_ESP32S3_LCKFB
	select SOC_ESP32S3_MINI_N8
	select SOC_ESP32S3_PROCPU if BOARD_ESP32S3_LCKFB_ESP32S3_PROCPU || BOARD_ESP32S3_LCKFB_ESP32S3_PROCPU_USB
	select SOC_ESP32S3_APPCPU if BOARD_ESP32S3_LUATOS_CORE_SIMPLE_ESP32S3_APPCPU || BOARD_ESP32S3_LCKFB_ESP32S3_APPCPU_USB
